{
  "ConnectionStrings": {
    "DefaultConnection": "Host=${API_DATABASE_HOST};Database=${API_DATABASE_DATABASE};Username=${API_DATABASE_USER};Password=${API_DATABASE_PASSWORD}"
  },
  "Encryption": {
    "DatabaseCipher": "${API_DATABASE_CIPHER}",
    "JWTToken": "${API_JWT_TOKEN}"
  },
  "Settings": {
    "DisableAccountCreation": false
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}